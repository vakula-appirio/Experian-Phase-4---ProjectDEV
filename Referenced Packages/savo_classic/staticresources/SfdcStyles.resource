html { 
	font-family: Arial, Helvetica, sans-serif;
	color: #666;
}

.panel {
	width: 320px;
	float: left;
	border: 1px solid #d3d3d3;
	border-radius: 4px;
	font-size: 0.9em;
	font-weight: normal;
	color: #585858;
	margin: 0px 10px 10px;
	text-align: left;
	font-family: Arial, Helvetica, sans-serif;
}
.panelCenter {
	width: 320px;
	border: 1px dotted #d3d3d3;
	border-radius: 4px;
	font-size: 0.9em;
	font-weight: normal;
	color: #585858;
	text-align: center;
	margin: 0px 0px 15px;
	font-family: Arial, Helvetica, sans-serif;
	padding: 3px 0;
}
.panelCenter-paging {
	width: 320px;
	float: left;
	border-radius: 4px;
	font-size: 12px;
	font-weight: bold;
	color: #585858;
	text-align: center;
	margin-top: 10px;
	margin-right: 0px;
	margin-bottom: 10px;
	margin-left: 0px;
	font-family: Arial, Helvetica, sans-serif;
	border-top-width: 0px;
	border-right-width: 0px;
	border-bottom-width: 0px;
	border-left-width: 0px;
	border-top-style: none;
	border-right-style: none;
	border-bottom-style: none;
	border-left-style: none;
	border-top-color: #d3d3d3;
	border-right-color: #d3d3d3;
	border-bottom-color: #d3d3d3;
	border-left-color: #d3d3d3;
}

.widgetheader{
	margin: 0;
	padding: 5px 0px;
	color: #333;
	background: #efefef;
	text-transform: uppercase;
	border-bottom: 1px solid #d3d3d3;
	height : 25px;
}

.widgetheader a.refresh{
	color: #333;
	font-size: 1.0em;
	text-transform: none;
	line-height:25px;
	float:right;
	margin-right: 8px;
}

.panel h1.label {
	font-size: 1em !important;
	margin: 0;
	color: #333;
	background: #efefef;
	text-transform: none;
  	line-height:25px;
}

.panel input.searchItem {
	width: 225px;
	margin-top: 0;
	vertical-align: middle;
}

.panel img.image {
	margin-left: 8px;
	height: 15px;
}

.postContent {
	padding: 0 10px;
}

.postTitle {
	margin-top: 5px;
	display: block;
}

/*** definition lists for list widget and search widget ***/
dl {
	margin: 10px;
}

dd { 
	margin-left: 40px; 
	color: #999; 
	font-size: .9em; 
}

dd#pop-up { 
	display: none; 
	position: absolute;
	padding: 10px; 
	background: #F4F4F4; 
	border: 1px solid #CCCCCC; 
	font-size: 100%;
	text-align: center;
	z-index: 1000 !important;
	-moz-border-radius: 5px 5px 0px 0px;
	border-radius: 5px 5px 0px 0px; 
}

dd h3 {
	margin-top: 5px;
	color: #333;
}

/*** asset titles ***/
.title { 
	margin-left: 40px; 
	font-weight: bold; 
	margin-bottom: 3px; 
}

.title:hover { 
	cursor: pointer; 
}

.title a {
	font-weight: bold;
	color: #333;
	text-decoration: none;
}

.title a.download { 
	color: #999; 
	font-weight: normal; 
	font-size: .8em; 
}

.icon { 
	height: 32px; 
	width: 32px; 
	float: left; 
}

