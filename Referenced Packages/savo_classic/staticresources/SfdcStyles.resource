/*** pop-up styles ***/


/*** Operation Icons ***/
.search_Image{
	background: url('/resource/1362087995000/savo_classic__IconMagnify') no-repeat -0px 0px;
	margin-top : 3px;
	float:right;
	margin-right: 5px;  
}

.refresh_Image{
	background: url('/resource/1362087995000/savo_classic__IconMagnify') no-repeat -14px 0px;
	margin-top : 3px;
	float:right;
	margin-right: 5px;
}

.operationIcon{
	height: 20px; 
	width: 15px;
	font-size: 1.0em;
	text-transform: none;
	line-height:25px;
}

.left {
	float: left;
}

.right {
	margin-left: 140px;
	text-align: left;
}

.right ul {
	list-style-type: none;
	margin: 0;
	padding: 0;
}


/*** search styles ***/
#searchResults {
	display: none;
}

#blankSearch {
	padding: 50px;
	color: #999;
	text-align: center;	
}

.search {
	margin-left: 30px;
	width: 400px !important;
}

.recommended {
	margin-left: 30px;
}

/*** default styles for post widgets ***/
.panel h1, h2, h3, h4, h5 {
	font-size: 1em;
	padding: 0px 10px;
}

.pagination {
    margin: 20px 0;
}

.pagination span {
    display: inline-block;
    font-size: 1em;
    margin-left: 0px;
    padding: 1px 1px 1px 4px !important;
    border-left: 1px solid #999;
}

.pagination .all_pages {
	padding-left: 0px !important;
}

.pagination span:first-child {
    border-left: none;
}

.pagination span.previous_page {
	text-align: center;
}

.pagination span.next_page {
   text-align: center;
}

.pagination span.active a {
    color: #0077A4;
    font-weight: bold;
    cursor: default;
}

.pagination span.disabled a {
    color: #999;
    cursor: default;
}

.pagination a {
    display: block;
    text-decoration: none;
}

.arrow-left {
    display: inline-block;
    width: 0;
    height: 0;
    margin-right: 3px;
    border-top: 4px solid transparent;
    border-bottom: 4px solid transparent;
    border-right: 4px solid #0077A4;
}

span.disabled .arrow-left {
    border-right: 4px solid #999;
}

.arrow-right {
    display: inline-block;
    *display: inline;
    zoom: 1;
    width: 0;
    height: 0;
    margin-left: 3px;
    border-top: 5px solid transparent;
    border-bottom: 5px solid transparent;
    border-left: 4px solid #0077A4;    
}

span.disabled .arrow-right {
    border-left: 4px solid #999;
}